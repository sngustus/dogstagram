<h1>Users</h1>

<table style="background-color: #00b5e2">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.password %></td>
      <td>'blank'</td>
      <td>'blank'</td>
      <td>'blank'</td>
    </tr>
  <% end %>

</table>

